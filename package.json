{
  "name": "@hospitalrun/components",
  "version": "0.16.0",
  "license": "MIT",
  "main": "dist",
  "module": "dist/components.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "scss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HospitalRun/components.git"
  },
  "scripts": {
    "commit": "npx git-cz",
    "dev": "tsdx watch",
    "build": "tsdx build",
    "prepublishOnly": "npm run build",
    "test": "tsdx test --env=jsdom",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"test/**/*.{js,jsx,ts,tsx}\" \"stories/**/*.{js,jsx,ts,tsx}\" --fix",
    "coveralls": "tsdx test --coverage --coverageReporters=text-lcov | coveralls",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook",
    "docgen": "genDocs.sh",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "bootstrap": "^4.3.1",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.14"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "~7.5.5",
    "@commitlint/cli": "~8.2.0",
    "@commitlint/config-conventional": "~8.2.0",
    "@commitlint/prompt": "~8.2.0",
    "@semantic-release/changelog": "~3.0.4",
    "@semantic-release/commit-analyzer": "~6.3.0",
    "@semantic-release/git": "~7.0.16",
    "@semantic-release/github": "~5.5.3",
    "@semantic-release/npm": "~5.2.0",
    "@semantic-release/release-notes-generator": "~7.3.0",
    "@storybook/addon-actions": "~5.1.11",
    "@storybook/addon-info": "~5.2.6",
    "@storybook/addon-links": "~5.1.11",
    "@storybook/addons": "~5.1.11",
    "@storybook/react": "~5.1.11",
    "@storybook/theming": "~5.1.11",
    "@types/chart.js": "~2.8.2",
    "@types/enzyme": "~3.10.3",
    "@types/jest": "~24.0.17",
    "@types/react": "~16.9.5",
    "@types/react-dom": "~16.9.1",
    "@types/sinon": "~7.0.13",
    "@types/storybook__addon-info": "~4.1.2",
    "@types/storybook__react": "~4.0.2",
    "@typescript-eslint/eslint-plugin": "~2.6.1",
    "@typescript-eslint/parser": "~2.6.1",
    "awesome-typescript-loader": "~5.2.1",
    "babel-loader": "~8.0.6",
    "bootstrap": "~4.3.1",
    "commitizen": "~4.0.3",
    "commitlint-config-cz": "~0.12.1",
    "coveralls": "~3.0.6",
    "cz-conventional-changelog": "~3.0.2",
    "enzyme": "~3.10.0",
    "enzyme-adapter-react-16": "~1.14.0",
    "eslint": "~6.6.0",
    "eslint-config-airbnb": "~18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "~2.18.2",
    "eslint-plugin-jest": "~23.0.2",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "~7.16.0",
    "eslint-plugin-react-hooks": "~1.7.0",
    "husky": "~3.0.5",
    "jest-canvas-mock": "~2.1.1",
    "lint-staged": "~9.2.5",
    "prettier": "~1.18.2",
    "react": "~16.10.2",
    "react-bootstrap": "~1.0.0-beta.14",
    "react-docgen": "~5.0.0-beta.1",
    "react-docgen-typescript-loader": "~3.2.0",
    "react-dom": "~16.10.2",
    "semantic-release": "~15.13.24",
    "sinon": "~7.5.0",
    "tsdx": "~0.9.0",
    "tslib": "~1.10.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "~1.2.25",
    "@fortawesome/free-solid-svg-icons": "~5.11.2",
    "@fortawesome/react-fontawesome": "~0.1.4",
    "chart.js": "^2.8.0",
    "formik": "^1.5.8",
    "moment": "^2.24.0",
    "react-spinners": "^0.6.1",
    "react-toastify": "^5.3.2"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock",
      "<rootDir>test-setup.ts"
    ]
  }
}
